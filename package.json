    "moment": "^2.21.0",
    "require-all": "^2.2.0",
    "slug": "^0.9.1",
    "sqlite": "^2.9.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^4.18.2",
    "glob": "^7.1.2",
    "nodemon": "^1.17.1",
