{
  "name": "auth",
  "description": "Authentication for Nginx auth request module",
  "version": "1.0.0",
  "author": "Sam Simmons <sam@samiconductor.com>",
  "license": "MIT",
  "main": "server.js",
  "repository": "samiconductor/auth",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development JWT_SECRET=jwt DB=auth.db HOST=localhost PORT=3000 TOKEN=token nodemon --config nodemon.json server",
    "setup": "cross-env NODE_ENV=development DB=auth.db node setup",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production JWT_SECRET=jwt DB=auth.db HOST=localhost PORT=3000 TOKEN=token node server",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "npm run lint -- --fix",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.2",
    "@hapi/good": "^8.2.0",
    "@hapi/good-console": "^8.1.0",
    "@hapi/good-squeeze": "^5.2.0",
    "@hapi/hapi": "^18.3.1",
    "@hapi/hoek": "^8.1.0",
    "@hapi/joi": "^15.1.0",
    "@nuxtjs/axios": "^5.5.4",
    "bcrypt": "^3.0.6",
    "camelcase": "^5.3.1",
    "cookie": "^0.4.0",
    "hapi-auth-jwt2": "^8.6.1",
    "inquire": "^0.4.8",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.8.1",
    "require-all": "^3.0.0",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "@mdi/font": "^3.8.95",
    "@nuxtjs/vuetify": "^1.1.1",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "jwt",
    "hapi",
    "auth",
    "nginx",
    "nuxt"
  ]
}
